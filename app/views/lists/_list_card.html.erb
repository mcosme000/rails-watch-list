<% @lists.each do |list| %>
  <div class="col-sm-12 col-md-6 col-lg-4">
    <div class="list-card shadow-sm"
    style="background-image: url(<%= list.photo.attached? ? cl_image_path(list.photo.key) : 'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTPQDYau3Hs4-xw1i8jVSUY4BlF4FLmg8lQqg&usqp=CAU' %>)">
      <p><%= link_to list.name, list_path(list) %></p>
    </div>
  </div>
<% end %>
